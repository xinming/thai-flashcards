%table.data_list
  %thead
    %tr
      %th.th_col Thai
      %th.en_col English
      %th.ph_col Pronounciation
      / %th Edit
  %tbody
    - vocabs.each do |vocab|
      %tr
        %td.thai=link_to vocab.thai, vocab
        %td=h vocab.english
        %td= vocab.print_pronunciation
        / %td
        /   = link_to edit_image, edit_vocab_path(vocab)
